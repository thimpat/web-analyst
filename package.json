{
  "name": "web-analyst",
  "version": "2.0.0",
  "description": "Web Analyst is a simple back-end tracking system to measure your web app performance.",
  "main": "index.cjs",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:all": "to-esm constants.cjs --output ./web/js && to-esm lib/common.cjs --output ./web/js",
    "run:stats": "npm run build:all && npx --yes genserve@latest restart stats.project --dir web --dir C:/projects/genserve/my-server.web --port 8080 --dir node_modules/tabulator-tables"
  },
  "repository": {
    "type": "git",
    "url": "git@my-centos-server.com:thimpat/node-statistics.git"
  },
  "keywords": [
    "tracking",
    "system",
    "statistics",
    "visits",
    "number",
    "of",
    "visitors",
    "web analyzer",
    "web analytics",
    "web analyst"
  ],
  "author": "Patrice THIMOTHEE <thimpat@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@thimpat/libutils": "^1.13.0",
    "chart.js": "^1.1.1",
    "tabulator-tables": "^5.4.1",
    "ua-parser-js": "^0.7.9"
  },
  "browser": {
    "Chartjs": "./node_modules/chart.js/Chart.js"
  },
  "style": [
    "./public/css/style.css"
  ],
  "devDependencies": {
    "eslint": "^8.25.0",
    "to-esm": "^2.24.1"
  }
}
